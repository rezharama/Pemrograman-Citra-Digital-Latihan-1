%mapout = changem(Z,newcode,oldcode)
drNew = changem(dr, [255, 73, 255, 73], [127, 255, 191, 242]);
dgNew = changem(dg, [255, 67, 255, 67], [127, 255, 191, 242]);
dbNew = changem(db, [255, 40, 255, 40], [127, 255, 191, 242]);


for i = 1:baris;
    for j = 1:kolom;
        d8New(i,j,1) = drNew(i,j);
        d8New(i,j,2) = dgNew(i,j);
        d8New(i,j,3) = dbNew(i,j);
    end
end


imwrite(d8, 'object.bmp');
citra = imread('object.bmp');

imwrite(d8New, 'objectBalik.bmp');
citraBalik = imread('objectBalik.bmp');

subplot(1,2,1), imshow(citra);
subplot(1,2,2), imshow(citraBalik);
