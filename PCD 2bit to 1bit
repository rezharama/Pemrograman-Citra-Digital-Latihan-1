        127 127 127 127 255 255 255 255 127 127 127 127 255 255 255 255 127 127 127 127];
    
db = [
        127 127 127 127 255 255 255 255 127 127 127 127 255 255 255 255 127 127 127 127;
        127 127 127 127 255 255 255 255 127 127 127 127 255 255 255 255 127 127 127 127;
